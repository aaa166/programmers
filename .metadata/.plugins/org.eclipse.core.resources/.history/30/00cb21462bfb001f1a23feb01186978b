package baekjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		String[] str = new String[n];
		
		for (int i = 0; i < n; i++) {
			str[i] = sc.next();
			System.out.println(i);
		}
		
//		System.out.println(Main.baekjoon10870(n));;
		
	}
//	---------------------------------------------------------------------------------
	
//	재귀의 귀재
	public static int baekjoon25501(int n) {
		if (n==0) return 0;
		else if(n==1) return 1;
		else return Main.baekjoon10870(n-1)+Main.baekjoon10870(n-2);
	}
//	피보나치수5
    public static int baekjoon10870(int n) {
    	if (n==0) return 0;
    	else if(n==1) return 1;
    	else return Main.baekjoon10870(n-1)+Main.baekjoon10870(n-2);
	}
//	팩토리얼2
    public static void baekjoon27433(int n, int answer) {
    	if (n <= 1) {
    		System.out.println(answer);
    		return;
    	}else {
    		answer = answer * n;
    		n--;
    		baekjoon27433(n, answer);
    	}
    	return;
    }
//    2차원배열 출력
    public static void printArray2(int[][] arr) {
    	for (int i = 0; i < arr.length; i++) {
    		for (int j = 0; j < arr[0].length; j++) {
    			System.out.print(arr[i][j] + " ");
    		}
    		System.out.println();
    	}
    	System.out.println();
    }
    public static void printArray2(char[][] arr) {
    	for (int i = 0; i < arr.length; i++) {
    		for (int j = 0; j < arr[0].length; j++) {
    			System.out.print(arr[i][j] + " ");
    		}
    		System.out.println();
    	}
    	System.out.println();
    }


}
